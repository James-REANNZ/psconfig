{
    "archives": {
        "archive-snmp": {
            "archiver": "snmptrap",
            "data": {
                "dest": "monitor.example",
                "security-name": "perfsonar",
                "trap-oid": "1.2.3.4.5.6.7.8"
            }   
            
        }
    },
    
   "addresses": {
        "host-a.example": { "address": "host-a.example" },
        "host-b.example": { "address": "host-b.example" },
        "host-c.example": { "address": "host-c.example" }
    },
    
    "groups": {
        "group-snmp": {
            "type": "list",
            "addresses": [
                { "name": "host-a.example" },
                { "name": "host-b.example" },
                { "name": "host-c.example" }
            ]
        }
    },
    
    "tests": {
        "test-snmp": {
            "type": "snmpget",
            "spec": {
                "version": 3,
                "dest": "$group::0",
                "polls": 1,
                "oid": "1.3.6.1.2.1.2.2.1.14"
            }
        }
    },
    
    "schedules": {
        "schedule-snmp": {
            "repeat": "PT10M"
        }
    },
    
    
    "tasks": {
        "task-snmp": {
            "group": "group-snmp",
            "test": "test-snmp",
            "schedule": "schedule-snmp",
            "archives": [ "archive-snmp" ]
        }
    }
    
}