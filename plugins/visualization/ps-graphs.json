{
    "type": "ps-graphs",
    "requires": {
        "test-type": ["throughput", "latency", "latencybg", "rtt"],
        "archive-type": ["esmond"]
    },
    "defaults": {
        "base-url": "/perfsonar-graphs/graphWidget.cgi",
        "params": {
            "tool": ""
        }
    },
    "vars": {},
    "http-get-opts": {
        "source": {
            "required": true,
            "condition": "{% row %}",
            "arg": "{% row %}"
        },
        "dest": {
            "required": true,
            "condition": "{% col %}",
            "arg": "{% col %}"
        },
        "displaysetsrc": {
            "condition": "1",
            "arg": "%row.displayset"
        },
        "displaysetdest": {
            "condition": "1",
            "arg": "%col.displayset"
        },
        "url": {
            "required": true,
            "condition": "1",
            "arg": "%maUrl"
        },
        "reverseurl": {
            "required": true,
            "condition": "1",
            "arg": "%maUrlReverse"
        },
        "tool": {
            "condition": "{% viz.params.tool %}",
            "arg": "{%  viz.params.tool %}"
        } 
    }
}